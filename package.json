{
  "name": "gatehouse",
  "version": "2.0.0",
  "description": "Authentication Monitor",
  "main": "src/index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "deploy:development": "npm install",
    "build": "npm run quality:lint && npm run build:typescript",
    "build:typescript": "node_modules/typescript/bin/tsc",
    "nonfunctional": "npm run nonfunctional:documentation",
    "nonfunctional:documentation": "mkdir -p doc && node_modules/typedoc/bin/typedoc --out doc/ src/index.ts",
    "quality": "npm run quality:lint && npm run quality:unit-tests",
    "quality:lint": "node_modules/tslint/bin/tslint --project tsconfig.json",
    "quality:unit-tests": "npm run build:typescript && node_modules/.bin/jest",
    "start": "node src/index.js",
    "test": "npm run quality:unit-tests",
    "trunk:commit": "npm run quality && git commit"
  },
  "keywords": [
    "monitoring",
    "management",
    "telemetry"
  ],
  "author": "Simon Janes",
  "license": "BSD-2-Clause",
  "dependencies": {
    "bcrypt": "^3.0.0",
    "bcryptjs": "^2.4.3",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "csurf": "^1.9.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "mongodb": "^3.1.3",
    "morgan": "^1.9.0",
    "prop-types": "^15.6.2",
    "pug": "^2.0.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/mongodb": "^3.1.4",
    "@types/node": "^10.7.1",
    "@types/react": "^16.4.11",
    "@types/react-dom": "^16.0.7",
    "@types/redux": "^3.6.0",
    "jest": "^23.5.0",
    "nodemon": "^1.18.3",
    "tachyons": "^4.11.1",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  }
}
